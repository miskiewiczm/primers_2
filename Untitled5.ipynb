   "source": [
    "def nucltoint(primer):\n",
    "    line = \"\"\n",
    "    trans = {\"A\":'1', \"C\":'2', \"T\":'3', \"G\":'4'}\n",
    "    for i in primer:\n",
    "        line = line + trans[i]+\",\"\n",
    "    return line"
   ]
